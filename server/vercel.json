{
  "functions": {
    "api/*.py": {
      "runtime": "python3.9",
      "maxDuration": 10
    },
    "websocket/*.py": {
      "runtime": "python3.9",
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/websocket",
      "destination": "/websocket/index.py"
    }
  ]
}